    "test": "jest"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "16.2.0",
    "react-native": "0.52.0",
    "react-native-calendars": "^1.17.0",
    "react-native-firebase": "^3.2.2",
    "react-native-vector-icons": "^4.5.0",
    "react-navigation": "^1.0.0-beta.27",
    "react-redux": "^5.0.6",
    "reactotron-react-native": "^1.14.0",
    "reactotron-redux": "^1.13.0",
    "reactotron-redux-saga": "^1.13.0",
    "redux": "^3.7.2",
    "redux-persist": "^5.4.0",
    "redux-saga": "^0.16.0",
    "reduxsauce": "^0.7.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.11.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "22.1.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-preset-react-native": "4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-react-native": "^3.2.1",
    "jest": "22.1.3",
    "react-dom": "^16.2.0",
    "react-test-renderer": "16.2.0",
    "redux-mock-store": "^1.4.0",
    "redux-saga-tester": "^1.0.378",
    "sinon": "^4.1.6"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation)"
    ],
    "setupFiles": [
      "<rootDir>/__tests__/setup.js"
    ],
    "testMatch": [
      "<rootDir>/__tests__/**/?(*.)(spec|test).js"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}",
      "src/pages/**/*.{js,jsx}",
      "src/store/ducks/**/*.{js,jsx}",
      "src/store/sagas/**/*.{js,jsx}"
    ]
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  }
}
